  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "jsdoc": "^3.4.3",
    "jsdoc-to-markdown": "^2.0.1",
    "mocha": "^3.2.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "uglify-js": "^2.7.5"
  }
